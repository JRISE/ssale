<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="login" namespace="/" extends="mypackage">
		<action name="userLoginAction_*" method="{1}" class="com.ssale.action.sys.UserLoginAction">
			<result name="login">${pageContext.request.contextPath}/login.jsp</result>
			<result name="index">${pageContext.request.contextPath}/WEB-INF/pages/home/index.jsp</result>
		</action>
	</package>
</struts>